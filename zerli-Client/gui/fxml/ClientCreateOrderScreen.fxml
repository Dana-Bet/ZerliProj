<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="599.0" prefWidth="909.0" style="-fx-background-image: url('/images/back6.JPG');" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlers.ClientCreateOrderController">
   <children>
      <Button fx:id="OurCatalogBtn" layoutX="398.0" layoutY="213.0" mnemonicParsing="false" onAction="#OpenCatalog" prefHeight="41.0" prefWidth="344.0" text="Our Catalog">
         <font>
            <Font name="Yu Gothic UI Light" size="24.0" />
         </font>
      </Button>
      <Button fx:id="MakeYourOwnBtn" layoutX="398.0" layoutY="319.0" mnemonicParsing="false" onAction="#MakeYourOwnPage" prefHeight="41.0" prefWidth="344.0" text="Make Your Own Product">
         <font>
            <Font name="Yu Gothic UI Light" size="24.0" />
         </font>
      </Button>
      <Button fx:id="CartBtn" layoutX="789.0" layoutY="19.0" mnemonicParsing="false" onAction="#OpenCartPage" prefHeight="61.0" prefWidth="110.0">
         <graphic>
            <ImageView fx:id="cartImage" fitHeight="42.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@shopping-cart-png-icon-free-download-604195.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="backBtn" layoutX="26.0" layoutY="535.0" mnemonicParsing="false" onAction="#backToMain" prefHeight="36.0" prefWidth="165.0" text="back to main">
         <font>
            <Font name="Yu Gothic UI Light" size="17.0" />
         </font>
      </Button>
      <Label fx:id="NumberOfProductLbl" layoutX="876.0" layoutY="57.0" prefHeight="23.0" prefWidth="19.0" textFill="#f80909" />
   </children>
</AnchorPane>
